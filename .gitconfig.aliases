[alias]
        a = add
        aa = add .
        b = branch
        cm = commit
        cmm = commit -m
        co = checkout
        cob = checkout -b
        d = diff
        lg = log --oneline --graph --all
        pl = pull
        ps = push
        s = status
        st = stash
        stp = stash pop
        unst = reset HEAD
        uncm = reset --soft HEAD~1
